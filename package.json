    "@types/rimraf": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@webgpu/types": "^0.1.64",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.0.5",
    "madge": "^8.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
